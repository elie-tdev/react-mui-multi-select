{
  "name": "react-mui-multi-select",
  "version": "0.1.0",
  "sideEffects": false,
  "author": "topfullstackdev <joujiyoshino.work@gmail.com>",
  "description": "Simple and lightweight Mui multiple select dropdown component with search function",
  "homepage": "https://github.com/topfullstackdev/react-mui-multi-select",
  "files": [
    "dist"
  ],
  "main": "./dist/index.umd.js",
  "module": "./dist/index.es.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js"
    }
  },
  "engines": {
    "node": ">=12"
  },
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com/topfullstackdev/multi-select.git"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build && tsc -p tsconfig.release.json",
    "pages": "vite build --config vite.config.pages.ts",
    "pages:build": "yarn build",
    "serve": "vite preview --config vite.config.pages.ts",
    "test": "jest",
    "eslint:fix": "eslint ./app ./lib --fix",
    "eslint:run": "eslint ./app ./lib",
    "format:fix": "prettier -w -c ./app ./lib",
    "format:run": "pretty-quick",
    "lint": "yarn format:run && yarn eslint:run",
    "lint-fix": "yarn run format:fix && yarn eslint:fix",
    "prepack": "yarn build",
    "pub:dry-run": "yarn npm publish . --access public --tag beta --dry-run",
    "pub": "yarn npm publish . --access public --tag beta",
    "clean": "rimraf ./dist ./node_modules"
  },
  "peerDependencies": {
    "@mui/material": ">=5",
    "@mui/styles": ">=5",
    "@mui/system": ">=5",
    "@mui/types": ">=5",
    "@mui/utils": ">=5",
    "react": ">=18",
    "react-dom": ">=18"
  },
  "devDependencies": {
    "@emotion/react": "^11.4.0",
    "@emotion/styled": "^11.3.0",
    "@internal/eslint-config": "1.0.0",
    "@internal/prettier-config-react": "1.0.0",
    "@mui/icons-material": "^5.0.0",
    "@mui/lab": "^5.0.0-alpha.41",
    "@mui/material": "^5.0.0",
    "@mui/styles": "^5.0.0",
    "@mui/system": "^5.0.0",
    "@mui/types": "^7.0.0",
    "@mui/utils": "^5.0.0",
    "@types/jest": "^27.0.1",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^4.26.1",
    "@typescript-eslint/parser": "^4.26.1",
    "@vitejs/plugin-react-refresh": "^1.3.1",
    "eslint": "^7.28.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-react": "^7.24.0",
    "formik": "^2.2.9",
    "fs-extra": "^10.0.0",
    "history": "^5.0.0",
    "jest": "^27.0.4",
    "prettier": "^2.1.1",
    "pretty-quick": "^3.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router": "6.0.0-beta.0",
    "react-router-dom": "6.0.0-beta.0",
    "rimraf": "3.0.2",
    "ts-jest": "^27.0.3",
    "tslib": "^2.2.0",
    "typescript": "^4.4.3",
    "vite": "^2.3.4"
  },
  "eslintConfig": {
    "extends": "@internal"
  },
  "prettier": "@internal/prettier-config-react",
  "packageManager": "yarn@3.1.0-rc.6",
  "license": "MIT",
  "keywords": [
    "react, multiSelect, multidropdown, material, multi-select, react-mui-multi-select"
  ]
}
